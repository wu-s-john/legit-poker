generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Minimal test table represented in Prisma.
// Only the requested columns exist in the DB table.
// We designate `foo` as the primary key so Prisma can migrate.
model Test {
  foo Int @id
  bar Int
  baz Int

  @@map("test") // actual table name in Postgres
}

